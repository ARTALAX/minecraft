<?xml version="1.8" encoding="UTF-8"?>
<Configuration status="WARN">
  <Appenders>
    <Console name="Console" target="SYSTEM_OUT">
      <PatternLayout pattern="%d{HH:mm:ss.SSS} [%t/%level]: %msg%n" />
    </Console>
  </Appenders>
  <Loggers>
    <Logger name="com.mojang.blaze3d.platform.GlDebug" level="off" additivity="false">
      <AppenderRef ref="Console"/>
    </Logger>
    <Root level="info">
      <AppenderRef ref="Console"/>
    </Root>
  </Loggers>
</Configuration>
